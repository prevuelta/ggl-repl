{
    "name": "Rune",
    "version": "0.0.1",
    "description": "SVG glyph creator using paper.js. Grid based design system to produce letter forms, icons etc then export as SVG from browser.",
    "main": "app.min.js",
    "scripts": {
        "flow": "flow",
        "start": "run-s  build run:client",
        "run:server": "nodemon -r esm ./server/server.js -i stored -i tmp --watch ./dist",
        "run:client": "browser-sync start --proxy 'http://localhost:7300' --watch ./dist --port 7200",
        "prewatch": "npm run build:css; npm run copy-statics;",
        "watch": "run-p copy-statics watch:js watch:css watch:html run:server run:client",
        "watch:js": "webpack --watch",
        "watch:css": "node-sass -w ./src/styles/main.scss ./dist/main.css",
        "watch:html": "watch 'npm run build:html'",
        "build": "echo 'building app...'; run-s build:js build:css copy-statics",
        "build:html": "cp ./src/index.html ./dist/index.html",
        "build:js": "webpack ",
        "build:css": "echo 'compiling stylesheets...'; node-sass ./src/styles/main.scss ./dist/styles/main.css",
        "copy-statics": "echo 'Copying statics...'; cp -R ./src/assets/ ./dist/assets/",
        "test": "nodemon -r esm -e example,js ./test/test.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/prevuelta/rune.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/prevuelta/rune/issues"
    },
    "homepage": "https://github.com/prevuelta/rune",
    "devDependencies": {
        "@babel/core": "^7.7.0",
        "@babel/plugin-proposal-class-properties": "^7.7.0",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-flow": "^7.0.0",
        "@babel/preset-react": "^7.7.0",
        "@webpack-cli/serve": "^0.1.8",
        "babel-loader": "^8.0.6",
        "browser-sync": "^2.26.7",
        "eslint": "^5.16.0",
        "node-sass": "^4.13.0",
        "nodemon": "^1.19.4",
        "npm-run-all": "^4.1.5",
        "raw-loader": "^3.1.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "glob": "^7.1.5",
        "gm": "^1.23.1",
        "npm-check-updates": "^3.2.0",
        "react": "^16.11.0",
        "react-ace": "^7.0.5",
        "react-dom": "^16.11.0",
        "react-draggable": "^3.3.2",
        "string.prototype.matchall": "^3.0.2",
        "svg2img": "^0.6.1",
        "svgo": "^1.3.2"
    }
}
