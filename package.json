{
  "name": "Rune",
  "version": "0.0.1",
  "description": "",
  "main": "app.min.js",
  "scripts": {
    "flow": "flow",
    "start": "run-s  build run:client",
    "run:server": "nodemon -r esm ./server/server.js -i stored -i tmp",
    "run:client": "browser-sync start -c bs-config.js",
    "prewatch": "run-s build:css copy-statics build:html",
    "watch": "run-p watch:js watch:css run:server run:client",
    "watch:js": "webpack --watch",
    "watch:css": "node-sass -w ./src/styles/main.scss ./build/main.css",
    "watch:html": "watch 'npm run build:html'",
    "build": "echo 'building app...'; run-s build:js build:css",
    "build:html": "cp ./src/index.html ./build/index.html",
    "build:js": "webpack ",
    "build:css": "echo 'compiling stylesheets...'; node-sass ./src/styles/main.scss ./build/main.css",
    "copy-statics": "echo 'Copying statics...'; cp -R ./src/assets/ ./build/assets/",
    "test": "nodemon -r esm -e example,js ./test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prevuelta/rune.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prevuelta/rune/issues"
  },
  "homepage": "https://github.com/prevuelta/rune",
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.7.0",
    "@webpack-cli/serve": "^0.2.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "eslint": "^6.6.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^3.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "glob": "^7.1.5",
    "gm": "^1.23.1",
    "npm-check-updates": "^3.2.0",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-dom": "^16.11.0",
    "react-draggable": "^4.1.0",
    "string.prototype.matchall": "^4.0.0",
    "svg2img": "git+https://github.com/prevuelta/node-svg2img.git",
    "svgo": "^1.3.2",
    "xmldom": "^0.1.27"
  }
